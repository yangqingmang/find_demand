# 公开配置（非敏感信息）
# 此文件可以安全提交到版本控制系统

# 应用配置
DEBUG=false
LOG_LEVEL=INFO
OUTPUT_DIR=data/results

# SERP 分析配置
SERP_CACHE_ENABLED=true
SERP_CACHE_DURATION=3600
SERP_REQUEST_DELAY=1
SERP_MAX_RETRIES=3
SERP_API_MONTHLY_LIMIT=250
SERP_API_RENEWAL_DAY=26
SERP_API_FAILURE_LIMIT=1
SERP_API_SKIP_ON_FAILURE=true
SERP_API_FAILURE_COOLDOWN_HOURS=24

# Google Ads API 版本
GOOGLE_ADS_API_VERSION=v15
